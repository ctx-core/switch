{
	"name": "@ctx-core/switch",
	"version": "11.0.1",
	"description": "ctx-core switch",
	"keywords": [
		"ctx-core",
		"switch"
	],
	"homepage": "https://github.com/ctx-core/switch#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/switch/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/switch/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.78",
		"@ctx-core/sass": "workspace:^12.1.1",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.110",
		"svelte": "3.44.1",
		"typescript": "^4.5.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}